@model List<MunicipalComplaint.Models.CustomerSignup>
@{
    ViewBag.Title = "GenerateCertificate";
    Layout = "~/Views/Shared/_Empty.cshtml";
}
<div class="row">
    <div class="col-md-2">
        <img src="~/Content/Images/logo.png" height="100" width="100" />
    </div>
    <div class="col-md-5">
        <h3 class="text-center text-primary">Employee Data Sheet</h3>
    </div>

</div>
<table class="table table-bordered mt-3" >
    <thead>
        <tr class="bg-dark text-white" style="background-color:dimgrey;color:white;">
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>CNIC</th>
            <th>Contact</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.UserId</td>
                    <td>@employee.Username</td>
                    <td>@employee.Email</td>
                    <td>@employee.CNIC</td>
                    <td>@employee.Contact</td>
                    <td>@employee.Address</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="6">No Records Availiable</td>
            </tr>
        }
    </tbody>
</table>